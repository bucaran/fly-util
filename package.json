{
  "name": "fly-util",
  "version": "3.9.2",
  "description": "Fly Utility Toolbox",
  "main": "dist/index.js",
  "dependencies": {
    "babel-core": "^5.6.15",
    "babel-runtime": "^5.4.7",
    "chokidar": "^1.0.3",
    "clor": "^1.0.0",
    "dateformat": "^1.0.11",
    "debug": "^2.2.0",
    "glob": "^5.0.10",
    "interpret": "^0.6.4",
    "mz": "^2.0.0",
    "prettyjson": "^1.1.2",
    "rimraf": "^2.4.0",
    "update-notifier": "^0.5.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.9",
    "co": "^4.5.4",
    "coffee-script": "^1.9.3",
    "eslint": "^0.21.2",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0"
  },
  "scripts": {
    "setup": "npm i && npm run test",
    "test": "npm run build && npm run lint && npm run harmony-test | tspec",
    "build": "babel --optional runtime src/ -d ./dist",
    "lint": "eslint src/",
    "harmony-test": "node --harmony --harmony_arrow_functions ./node_modules/tape/bin/tape test/*.js",
    "deploy": "npm run test && git push origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flyjs/fly-util.git"
  },
  "keywords": [
    "fly",
    "utilities"
