  "bugs": {
    "url": "https://github.com/flyjs/fly-util/issues"
  },
  "engines": {
    "iojs": ">= 1.0.0",
    "node": ">= 0.12.0"
  },
  "homepage": "https://github.com/flyjs/fly-util"
}
